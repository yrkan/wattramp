    <string name="tutorial_results_save">Appuyez SAUVEGARDER pour mettre à jour FTP et zones</string>

    <!-- Page 8: Tips -->
    <string name="tutorial_tips_title">CONSEILS POUR RÉUSSIR</string>
    <string name="tutorial_tip1">Calibrer le capteur de puissance avant le test</string>
    <string name="tutorial_tip2">Échauffement de 10–15 minutes</string>
    <string name="tutorial_tip3">Rester hydraté - eau à portée</string>
    <string name="tutorial_tip4">Utiliser le mode ERG sur home-trainer</string>
    <string name="tutorial_tip5">Tester reposé, pas après des entraînements durs</string>
    <string name="tutorial_tip6">Pour RAMPE: pousser jusqu\'à épuisement</string>
    <string name="tutorial_tip7">Retester toutes les 4–6 semaines</string>

