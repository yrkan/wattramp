    <!-- Page 8: Tips -->
    <string name="tutorial_tips_title">成功秘诀</string>
    <string name="tutorial_tip1">测试前校准功率计</string>
    <string name="tutorial_tip2">10-15分钟热身</string>
    <string name="tutorial_tip3">保持水分 - 准备好水</string>
    <string name="tutorial_tip4">在智能骑行台上使用ERG模式</string>
    <string name="tutorial_tip5">休息后测试，避免高强度训练后</string>
    <string name="tutorial_tip6">递增测试：坚持到无法继续</string>
    <string name="tutorial_tip7">每4-6周重新测试</string>

